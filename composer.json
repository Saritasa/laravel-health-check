{
    "name": "saritasa/laravel-healthcheck",
    "authors": [
        {
            "name": "Eugene Mosunov",
            "email": "eugene.mosunov@saritasa.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/database": "^5.6 || 6.*",
        "illuminate/routing": "^5.6 || 6.*",
        "illuminate/support": "^5.6 || 6.*",
        "saritasa/php-common": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4" : {
            "Saritasa\\LaravelHealthCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saritasa\\LaravelHealthCheck\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\LaravelHealthCheck\\HealthCheckServiceProvider"
            ]
        }
    }
}
